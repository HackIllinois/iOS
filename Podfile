    use_frameworks!
    pod 'lottie-ios'
    pod 'SwiftKeychainAccess', :git => 'https://github.com/rauhul/keychain.git', :commit => 'a52855150f470b1942caa4bd28754c643a9fe110'
end

target 'HIAPI' do
    use_frameworks!
    pod 'APIManager', '~> 0.3.0'
end
